@article{dreier1999,
	Abstract = {In chapter five of Wise Choices, Apt Feelings Allan Gibbard develops what he calls a \textquoteleft{}normative logic\textquoteright intended to solve some problems that face an expressivist theory of norms like his. The first is \textquotedblleft{}the problem of embedding: The analysis applies to simple contexts, in which it is simply asserted or denied that such-and-such is rational. It says nothing about more complex normative assertions.\textquotedblright{}1 That is the problem with which I will be concerned. Though he doesn\textquoteright{}t list it as one of the problems to be solved, Gibbard\textquoteright{}s devices also explain what it means to say that a certain normative argument is \textquoteleft{}valid\textquoteright, that one normative statement follows from some others. Both of these questions arise because of the ways we ordinarily think and speak, ways which on a simplistic version of expressivism appear not to make any sense. An expressivist committed to making sense of the way we ordinarily think and speak in normative language owes some account of embeddings and of logic. Gibbard\textquoteright{}s device seems to me the clearest and most systematic of any expressivist attempt to meet these challenges.},
	Author = {Dreier, James},
	Date-Added = {2012-06-28 10:37:08 +0000},
	Date-Modified = {2012-06-28 10:37:38 +0000},
